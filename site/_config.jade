mixin footer
  footer
    .container
      .col4.col-m12#arte
        h1 Arte y Diseño Camila 
      .col4.col-m12
        .logoFooter
          img(src='site/img/logo.png', alt="Alex Courcelle")
      .col4.col-m12
        .container
            .col3
                img(src='site/img/twit.png', alt="twit")
            .col3
                img(src='site/img/facebook.png', alt="face")
            .col3
                img(src='site/img/youtube.png', alt="Youtube")
            .col3
                img(src='site/img/in.png', alt="in")